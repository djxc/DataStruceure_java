<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
        
	<bean id="dj" class="firstProgect.TellName">
		<property name="name" value="xc_dj"/>
	</bean>
	
	<bean id="MD" class="firstProgect.MoveDisk"/>
	
	<bean id="UD" class="firstProgect.UDisk"/>
	
	<bean id="c" class="firstProgect.Computer">	
		<property name="usb" ref="UD"></property>
	</bean>
	
	<bean id="connUtil" class="firstProgect.ConnectUtil">
		<property name="name" value="dj"/>
		<property name="password" value="123dj321"/>
		<property name="url" value="jdbc:mysql://localhost:3306/djdb"/>
		<property name="driver" value="com.mysql.jdbc.Driver"/>
	</bean>
	
	<bean id="UserDao" class="firstProgect.UserDaoImpl">
		<property name="connUtil" ref="connUtil"></property>
	</bean>
	
</beans>